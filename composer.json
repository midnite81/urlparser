{
    "name": "midnite81/urlparser",
    "description": "A PHP 7.1+ package to parse urls",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Rogers",
            "email": "webdev@centralblue.co.uk"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.2",
        "illuminate/support": "^5.7",
        "phpunit/phpunit": "^7.5",
        "satooshi/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Midnite81\\UrlParser\\": "src/",
            "Midnite81\\UrlParser\\Tests\\": "tests/"
